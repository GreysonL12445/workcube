//Code for work cube(box thingy)//

#include <PRIZM.h>   //Link up the library//
PRIZM blyat;         //Declare Object (reference to prizm)//

void setup(){
    blyat.PrizmBegin();
    blyat.setMotorPower(1,100);  //full speed
    delay(2000);
    blyat.setMotorPower(1,125);  //125 = brake
    delay(2000);
}
void loop(){
}

